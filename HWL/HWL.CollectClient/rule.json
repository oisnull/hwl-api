[
  {
    "Description": "beyondsoft",
    "Url": "https://www.beyondsoft.com/",
    "Level": 3,
    "Enable": false,
    "Rules": [
      {
        "Key": "title",
        "IsList": false,
        "Xpath": {
          "XpathRule": "//div[@class='detial_01']/h1[1]",
          "ExtractType": "Text"
        }
      },
      {
        "Key": "date",
        "IsList": false,
        "Xpath": {
          "XpathRule": "//div[@class='lab clearfix mrg-btm20']/span[2]",
          "ExtractType": "Text"
        },
        "Filter": {
          "DefaultValues": null,
          "RemoveChar": {
            "IsRegex": false,
            "Chars": [ "时间：", "Time：" ]
          }
        }
      },
      {
        "Key": "images",
        "IsList": true,
        "Xpath": {
          "XpathRule": "//div[@class='detial_01']//img/@src",
          "XpathEndAttributes": [ "src" ]
        }
      }
    ]
  },
  {
    "Description": "cnn",
