    "Level": 2,
    "Enable": false,
    "Rules": [
      {
        "Key": "title",
        "IsList": false,
        "Xpath": {
          "XpathRule": "//h1[@class='pg-headline']",
          "ExtractType": "Text"
        }
      },
      {
        "Key": "keys",
        "IsList": false,
        "Rule": {
          "DefaultValue": "CNN NEW"
        }
      },
      {
        "Key": "images",
        "IsList": true,
        "Xpath": {
          "XpathRule": "//section[@id='body-text']//img[@src]",
          "XpathEndAttributes": [ "src" ]
        }
      },
      {
        "Key": "content",
        "IsList": false,
        "Xpath": {
          "XpathRule": "//section[@id='body-text']"
        },
        "Filter": {
          "DefaultValues": null,
          "RemoveChar": {
            "IsRegex": true,
            "Chars": [
              "<[a|A]\\s*[^>]*>(.*?)</[a|A]>",
              "<cite\\s*[^>]*>(.*?)</cite>",
              "<div class=\"zn-body__read-more-outbrain\">.*</div>"
            ]
          }
        }
      },
      {
        "Key": "author",
        "IsList": false,
        "Xpath": {
          "XpathRule": "//span[@class='metadata__byline__author']",
          "ExtractType": "Text"
        }
      }
    ]
  },
