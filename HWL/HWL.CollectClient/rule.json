      {
        "Key": "official",
        "IsList": false,
        "Xpath": {
          "XpathRule": "//div[@class='post_time_source']/a[1]",
          "ExtractType": "Text"
        }
      },
      {
        "Key": "author",
        "IsList": false,
        "Xpath": {
          "XpathRule": "//span[@class='ep-editor'][1]",
          "ExtractType": "Text"
